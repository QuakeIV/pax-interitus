QTabWidget::pane {
    position: absolute;
    top: -1px; /* allow the tabs to overlap over the top border and join with the window below*/
}
QTabWidget::tab-bar {
  left: 5px; /* move to the right by 5px */
}
QTabBar {
  background: rgb(38, 44, 59); /* captures the margin above unselected tabs */
  /* the rest of the background color of the tab bar is handled by the containing widget on an individual basis*/
}
QTabBar::tab {
  background: rgb(72, 98, 145);
  border: 1px solid #9B9B9B;
  min-width: 8ex;
  padding: 2px;
  margin-right: -1px; /* overlap on borders so they arent doubled up */
}
QTabBar::tab:last {
  margin-right: 0px; /* prevent rightmost border from being chopped off */
}
QTabBar::tab:selected {
  background: rgb(44, 86, 145);
  border-color: rgb(105, 156, 232);
  border-bottom-color: rgb(44, 86, 145);
}
QTabBar::tab:!selected {
  margin-top: 3px; /* make non-selected tabs look smaller */
  /*border-bottom: 0px;*/
}</string>
